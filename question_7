# assignment-5
package Assignment_5;

public class question7 {

	public static void main(String[] args) {
/*Write a Java program to print all the numbers between 1 and 100
 *  (including 1 and 100) which are divisible by 5.	
 */
		
		for(int i=0;i<=100;i++)
		{
			if(i%5==0) {
			System.out.print(i+",");
			}
		}
	
		
		
		
		

	}

}
